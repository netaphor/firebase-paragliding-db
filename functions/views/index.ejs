<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Paragliding Dashboards</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
    <div class="container-fluid mt-3">
        <div class="row row-cols-1 row-cols-md-3 g-4">
            <% turnpoints.forEach(function(tp) { %>
                <% days.forEach(function(day) { %>
                    <div class="col">
                        <div class="card">
                            <img src="https://app.stratus.org.uk/blip/graph/blip_main.php?day=<%= day %>&tp=<%= tp %>" 
                                 class="card-img-top img-fluid" 
                                 alt="Blip graph for <%= tp %> on day <%= day %>">
                            <div class="card-footer text-center">
                                <small class="text-muted">Turnpoint: <%= tp %> | Day: <%= day %></small>
                            </div>
                        </div>
                    </div>
                <% }); %>
            <% }); %>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html></div>